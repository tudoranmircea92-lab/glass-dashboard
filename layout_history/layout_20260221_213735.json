{
  "version": 1,
  "app_title": "Glass Coating Industrial AI Dashboard",
  "dataset": {
    "file": "db2026.02.19.parquet",
    "date_column": "data"
  },
  "global": {
    "default_rows": 50000,
    "default_sample_mode": "head"
  },
  "tabs": [
    {
      "name": "Overview",
      "mode": "custom",
      "filters": [
        "product",
        "date",
        "thickness_mm"
      ],
      "panels": [
        {
          "type": "kpis",
          "title": "Quick KPIs",
          "metrics": [
            "thickness_mm",
            "glassThickness_mm",
            "actProcessSpeed_mm",
            "c27.actVacuumPressure",
            "c27.actVoltage",
            "L_ABS_mean",
            "RT_T_mean"
          ]
        },
        {
          "type": "chart",
          "title": "Thickness vs Process Speed",
          "chart_type": "scatter",
          "x": "actProcessSpeed_mm",
          "y": "thickness_mm",
          "color": "product"
        },
        {
          "type": "timeseries",
          "title": "Optical Mean Over Time",
          "date": "data",
          "y": "L_T_mean",
          "group": "product",
          "agg": "mean",
          "rolling": 7
        }
      ]
    }
  ]
}